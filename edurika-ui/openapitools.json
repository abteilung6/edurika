{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.11.0",
    "generators": {
        "v1.0": {
          "generatorName": "typescript-axios",
          "output": "./src/generated-api",
          "inputSpec": "http://127.0.0.1:8000/openapi.json",
          "additionalProperties": {
            "npmName": "restClient",
            "supportsES6": "true",
            "withInterfaces": true
          }
        }
      }
  }
}
